%script#app-template{:type => "text/template"}
  #my-projects
    %h1 PROJECTS
%script#project-list-template{:type => "text/template"}
  %ul.project-link
    %li
      -# %img{:src => "{{imageThumb}}", :alt => ""}
      %img{:src => lorem.image('300x200'), :alt => ""}
    %li
      %h3 {{ title }}
%script#project-template{:type => "text/template"}
  #project
    %h2 {{ title }}
    %h3
      %a{:href => "{{ link }}"} {{ link }}
    %img{:alt => "{{ title }}", :src => "{{ imageLarge }}"}
      %p {{ summary }}
      %p {{ detail }}
      %ul {{ builtWith }}
      %ul {{ collaborators }}
%script#bio-template{:type => "text/template"}
  #bio
    %h2 About me
    %p
      Studied Web Development Immersive at General Assembly Sydney and Graphic Design at Design Centre Enmore.
    %h3 Skills:
    %ul#skills-list
      %li Ruby on Rails
      %li Coffeescript
      %li SASS
      %li Backbonejs
%script#contact-template{:type => "text/template"}
  #contact
    %h2 Contact me!
    %p You can find me at the following places:
    %ul#contact-list
      %li
        %a{:href => "http://github.com/lyntco"} Github
      %li
        %a{:href => "http://blog.lynt.co"} Blog
      %li
        %a{:href => "http://twitter.com/strawz"} Twitter
    %h3 Want to grab a coffee?
    %p You can reach me at
    %p 0432 871 070
    %p or
    %p strawww@gmail.com!