%script#app-template{:type => "text/template"}
  #my-projects
    %h1 PROJECTS
%script#project-list-template{:type => "text/template"}
  %ul.project-link
    %li
      -# %img{:src => "{{imageThumb}}", :alt => ""}
      %img{:src => lorem.image('300x200'), :alt => ""}
    %li
      %h3 {{ title }}
%script#project-template{:type => "text/template"}
  #project
    %h2 {{ title }}
    %h3
      %a{:href => "{{ link }}"} {{ link }}
    %img{:alt => "{{ title }}", :src => "{{ imageLarge }}"}
      %p {{ summary }}
      %p {{ detail }}
      %h3 Built with:
      %ul
        {{#each builtWith }}
        %li {{ this }}
        {{/each }}
      %h3 Team:
      %ul
        {{#each collaborators }}
        %li
          %a{:href => "{{link}}"} {{ name }}
        {{/each }}
%script#bio-template{:type => "text/template"}
